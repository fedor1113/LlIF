<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-alan">
    <comment>Alan Interactive Fiction file format - game data</comment>
    
    <magic priority="50">
        <match type="string" offset="0" value="ALAN\x03"/>
    </magic>
    
    <glob pattern="*.acd"/>
    <glob pattern="*.a3c"/>
  </mime-type>
</mime-info>

